{
  "name": "actiontracker",
  "version": "1.3.1",
  "author": "Mike Pritchard <mike@arsenicsoup.com>",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "release": "standard-version && yarn run build && yarn run deploy",
    "deploy": "s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket actiontracker.org --profile s3-admin"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.11.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "papaparse": "^5.1.1",
    "vue": "^2.6.10",
    "vue-good-table": "^2.18.1",
    "vue-mq": "^1.0.1",
    "vue2-google-maps": "^0.10.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^8.0.2",
    "standard-version": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
